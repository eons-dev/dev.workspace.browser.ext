{
	"name": "eons-dev-launcher",
	"version": "0.0.0",
	"description": "Injects an 'Open' button on git pages to launch an Eons workspace.",
	"scripts": {
		"dev": "vite",
		"build": "pnpm run prebuild && vite build",
		"prebuild": "pnpm run prebuild:manifest && pnpm run prebuild:icons",
		"prebuild:manifest": "node sh/copy-manifest.js",
		"prebuild:icons": "node sh/copy-icons.js",
		"build:chrome": "BROWSER=chromium pnpm run build",
		"build:firefox": "BROWSER=gecko pnpm run build"
	},
	"devDependencies": {
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vite-plugin-web-extension": "^4.4.3"
	},
	"dependencies": {
		"liquidjs": "^10.24.0",
		"webextension-polyfill": "^0.12.0"
	}
}